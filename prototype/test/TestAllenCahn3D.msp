post_pro:
  - post_processing:
     calculation_path: "Problem"
     main_folder_path: "Saves"

input_data: 
  - generate_h1_cube:
     refinement_level: 0
     grid_dim: [30,30,30]
     discretization: [1e-3, 1e-3, 1e-3]
  - set_h1_bcs:
     names:  [rear,      lower,     right,   upper,      left,   front]
     types:  [Neumann, Neumann, Dirichlet, Neumann, Dirichlet, Neumann]
     values: [0.,           0.,        1.,      0.,        0.,      0.]
  - set_variable_test1:
     center: [0,0,0]    
     no_idea: [1,0,0]
     thickness: 5.e-5
     radius: 5.e-4
     epsilon: 5.e-4
     field_name: "phi"

problem:
  - set_h1_allen_cahn_problem:
     crit_cvg: 1.e-12
     mobility: 1.e-5
     epsilon: 5.e-4
     sigma: 6.e-2
     name: "AllenCahn"

coupling:
  - allen_cahn_h1_default_coupling

compute_loop:
  - set_time_discretization_3d
  - solve_3d

global:
  paraview_frequency: 1
  verbosity: 1
  final_time: 5.0
  dt: 1.
